<input type=\"color\" name=\"Shad\" title=\"Form Shadow Color\" value=\"#232323\" selected>\n        <input type=\"text\" title=\"Login Page Url\" name=\"LoginUrl\" value=\"accedi.html\">\n        <input type=\"text\" title=\"Signup Page Url\" name=\"SignupUrl\" value=\"registrati.html\">\n        <input type=\"text\" title=\"Reset Page Url\" name=\"ResetUrl\" value=\"reset.html\">\n        <input type=\"text\" title=\"Change pass. Page Url\" name=\"ChangeUrl\" value=\"recupero.html\">\n        <header>Background</header>\n    <fieldset type=\"background\" name=\"bg\" parallax>\n        <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/background.jpg\" parallax>\n        <input type=\"color\" title=\"Background Color\" value=\"#efefef\">\n        <input type=\"video\" title=\"Background Video\" value=\"https://youtu.be/hlFJcmEMv0s\" selected>\n    </fieldset>\n    <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n    <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#232323\" condition=\"overlay && bg.type !== 'color'\">\n    <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"overlay && bg.type !== 'color'\">\n\n    <!-- End block parameters -->\n    </mbr-parameters> \n<div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n  </div>\n  <div class=\"container-fluid\">\n    <div class=\"wrapper bg-white\" mbr-if=\"Ftype == 'Login'\">\n    <div class=\"h2 text-center mbr-fonts-style\" mbr-text mbr-if=\"showTitle\" mbr-theme-style=\"display-2\"><span style=\"font-style: normal;\">Accedi</span></div>\n    <div class=\"h4 text-muted text-center pt-2  mbr-fonts-style\" mbr-text mbr-if=\"showsubTitle\" mbr-theme-style=\"display-4\">Enter your login details</div>\n    <form class=\"pt-3\" mbr-class=\"{'usermanager_form': Upt == 'Basic', 'usermanager_advance_form': Upt == 'Advance'}\">\n        <div class=\"row\">\n                        <div hidden=\"hidden\" class=\"alert alert-success col-12 text-center\"></div>\n                        <div hidden=\"hidden\" class=\"alert alert-danger col-12 text-center\"></div>\n\t\t\t\t\t\t<input type=\"hidden\" name=\"form_name\" value=\"loginform\">\n                    </div>\n        <div class=\"form-group py-2\">\n            <div class=\"input-field\" mbr-if=\"Mlf == 'email'\">\n                 <span class=\"mbr-iconfont fas fa-fw fa-envelope p-2 pr-3\"></span> \n            <input type=\"text\" placeholder=\"Email Address\" name=\"email\" required>\n            </div>\n            <div class=\"input-field\" mbr-if=\"Mlf == 'username'\">\n                 <span class=\"mbr-iconfont fas fa-fw fa-user p-2 pr-3\"></span> \n            <input type=\"text\" placeholder=\"Username\" name=\"username\" required>\n            </div>\n        </div>\n        <div class=\"form-group py-1 pb-2\">\n            <div class=\"input-field\"> <span class=\"mbr-iconfont fas fa-fw fa-lock p-2 pr-3\"></span> <input type=\"password\" placeholder=\"Enter your Password\" name=\"password\" required class=\"pass-input\"> <a href=\"#showpass\" class=\"passshow bg-white text-muted btn-show-pass\"> <span class=\"pass mbr-iconfont  far fa-fw fa-eye-slash\"></span> </a> </div>\n        </div>\n        <div class=\"d-flex align-items-start\">\n            <div class=\"remember\"><label class=\"option text-muted\"> Remember me <input type=\"radio\" name=\"radio\"> <span class=\"checkmark\"></span> </label> </div>\n            <div class=\"ml-auto ms-auto\"> <a href=\"{{ResetUrl}}\" id=\"forgot\">Forgot Password?</a> </div>\n        </div> \n        <div class=\"d-grid gap-2\">\n        <button class=\"btn btn-block text-center my-3 py-2 mainbtn\">Log in</button>\n        <a href=\"#loading\" class=\"btn btn-block text-center my-3 py-2 loadi